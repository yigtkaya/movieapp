{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": ["--flavor", "dev", "--dart-define=API_KEY=your_api_key_here"]
    },
    {
      "name": "Staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_stg.dart",
      "args": ["--flavor", "stg", "--dart-define=API_KEY=your_api_key_here"]
    },
    {
      "name": "Production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_prod.dart",
      "args": ["--flavor", "prod", "--dart-define=API_KEY=your_api_key_here"]
    }
  ]
}
